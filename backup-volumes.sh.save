#!bin/sh
CONTAINER_ID=$1
CONTAINER_NAME=$2
PATH=$3
if [ -z $CONTAINER_ID ]
then
   echo "Error : Missing container id parameters"
   exit 1
fi

if [ -z $CONTAINER_NAME ]
then
   echo "Error : Missingy Container name parameters"
   exit 1
fi

if [ -z $PATH ]
then
   $PATH = "./data-backup"
fi

/usr/bin/docker 
